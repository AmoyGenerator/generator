<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.jmr.entity">
	<class name="Functions" table="Functions">
		<id name="id" type="java.lang.Integer">
			<column name="functionsid" />
			<generator class="identity" />			
		</id>
		<property name="name" type="java.lang.String">
			<column name="name" />
		</property>	
		<property name="link" type="java.lang.String">
			<column name="link" />
		</property>	
		<property name="sort" type="java.lang.Integer">
			<column name="sort" />
		</property>	
		<property name="topfuncid" type="java.lang.Integer">
			<column name="topfuncid" />
		</property>			
		<property name="level" type="java.lang.Integer">
			<column name="level" />
		</property>	
		
	</class>
</hibernate-mapping>
