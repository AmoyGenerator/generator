<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.jmr.entity">
	<class name="Codedesc" table="Codedesc">
		<composite-id>
	        <key-property name="type" column="type" type="java.lang.String" />
	        <key-property name="code" column="code" type="java.lang.String"/>
	        
		</composite-id>
		<property name="name" type="java.lang.String">
			<column name="name" />
		</property>	
		<property name="sort" type="java.lang.Integer">
			<column name="sort" />
		</property>
	</class>
</hibernate-mapping>
