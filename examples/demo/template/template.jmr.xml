<?xml version="1.0" ?>
<jmr xmlns="http://www.eclipse-jmr.org/jmr-1.0.xsd">
 <run>
      <project name="start">
         <directory name="hello">
            <task name="hello">
               <modelGroupName></modelGroupName>
               <actionClass></actionClass>
               <templatePath>demo/template/start/hello.jet</templatePath>
               <targetContainer>/demo/template/start</targetContainer>
               <targetName>hello</targetName>
               <targetExt>txt</targetExt>
               <targetEncoding>UTF-8</targetEncoding>
               <generation mode="overlay"></generation>
            </task>
         </directory>
      </project>  
