               <targetName>expression</targetName>
               <targetExt>txt</targetExt>
               <targetEncoding>UTF-8</targetEncoding>
               <generation mode="overlay"></generation>
            </task>
         </directory>
      </project>
      <project name="object">
         <directory name="out">
            <task name="out">
               <modelGroupName></modelGroupName>
               <actionClass></actionClass>
               <templatePath>demo/template/object/out.txt.jet</templatePath>
               <targetContainer>/demo/template/object</targetContainer>
               <targetName>out</targetName>
               <targetExt>txt</targetExt>
               <targetEncoding>UTF-8</targetEncoding>
