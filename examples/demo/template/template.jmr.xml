            </task>
         </directory>
         <directory name="context">
            <task name="var">
               <modelGroupName></modelGroupName>
               <actionClass>action.MyAction</actionClass>
               <templatePath>demo/template/object/context/var.txt.jet</templatePath>
               <targetContainer>/demo/template/object/context</targetContainer>
               <targetName>var</targetName>
               <targetExt>txt</targetExt>
               <targetEncoding>UTF-8</targetEncoding>
